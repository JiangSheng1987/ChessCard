{
  "extends": "airbnb/base",
  "env":{
    "node":true,
    "es6":true
  },
  "rules": {
    "no-param-reassign": "off",
    "no-underscore-dangle": "off",
    "brace-style": "off",
    "max-len": "off",
    "indent": ["error", 4],
    "arrow-body-style": ["error", "always"],
    "no-restricted-syntax": "off",
    "guard-for-in": "off",
    "key-spacing": "off",
    "global-require": "off",
    "import/no-unresolved": 0,
    "eqeqeq": "off",
    "eol-last": "off",
    "prefer-rest-params": "off"
  },
  "globals": {
    "window": true,
    "Math": true,
    "WebSocket": true,
    "Global": true,
    "WebSocketManager": true,
    "HttpRequestManager": true,
    "NativeExtensionManager": true,
    "Tools": true,
    "Animation": true,
    "proto": true,
    "goog": true,
    "ReconnectingWebSocket": true,
    "clone": true,
    "md5": true,
    "cc": true,
    "gl": true,
    "CC_JSB": true,
    "jsb": true
  }
}